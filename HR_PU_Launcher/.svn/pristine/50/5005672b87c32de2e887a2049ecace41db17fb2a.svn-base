<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >
	
    <Button   style="@style/font_40_black"
	        android:id="@+id/bt_all"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="@string/str_finish"/>
    
    <Button   style="@style/font_41"
	        android:id="@+id/bt_rse"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toRightOf="@id/bt_all"
	        android:layout_marginLeft="100dip"
	        android:visibility="gone"
	        android:text="@string/str_rse"/>
    
    <Button   style="@style/font_40_black"
	        android:id="@+id/bt_dvd"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toRightOf="@id/bt_rse"
	        android:layout_marginLeft="100dip"
	        android:visibility="gone"
	        android:text="@string/str_dvd"/>
    
    <Button   style="@style/font_40_black"
	        android:id="@+id/bt_clear"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:text="@string/str_clear"/>
    
    <Button   style="@style/font_40_black"
	        android:id="@+id/bt_init"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toRightOf="@id/bt_rse"
	        android:layout_marginLeft="300dip"
	        android:text="@string/str_init"/>
    
    <TextView   style="@style/font_35"
        android:id="@+id/tv_fwdn"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/bt_clear"
        android:layout_toRightOf="@id/bt_all"
        android:layout_marginLeft="20dip"
        android:textColor="#ff8484"
        android:visibility="invisible"
        android:text="@string/str_fwdn"/>
    
        <ImageView 
        android:id="@+id/tv_barcode"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_toRightOf="@id/tv_fwdn"
        android:background="#FF0000"
        android:visibility="invisible"
        />
    
     <TextView   style="@style/font_35"
         android:id="@+id/tvCasID"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_fwdn"
        android:layout_toRightOf="@id/bt_all"
        android:layout_marginLeft="20dip"
        android:textColor="#8ec96d"
        />
	
	<TextView   style="@style/font_35" 
	    android:id="@+id/tv_Version"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="10dp"
	    android:textColor="#ff8484"
	    android:layout_below="@id/tvCasID"/>
	    
    <TextView 
        android:id="@+id/tv_center"
        android:layout_width="30dip"
        android:layout_height="1dip"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        />
    
    <ListView 
	    android:id="@+id/list_view"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_toLeftOf="@id/tv_center" 
	    android:layout_below="@id/tv_Version"/>
    <ListView 
	    android:id="@+id/list_view1"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_toRightOf="@id/tv_center" 
	    android:layout_below="@id/tv_Version"/>
    
    
    

</RelativeLayout>
