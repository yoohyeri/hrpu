    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="peace.tabbutton.Fragment1" >
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/setting_list"
            android:layout_width="@dimen/list_w"
            android:layout_height="match_parent" >

            <include layout="@layout/_kia_layout_setting_list" />
        </RelativeLayout>

        <FrameLayout
            android:id="@+id/sub_fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#0d0d0d" >
        </FrameLayout>
    </LinearLayout>
</FrameLayout>